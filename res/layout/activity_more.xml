<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_weight="1"
    android:background="@color/white"
    >
    <ImageButton
        android:id="@+id/imageButtonThink"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerInParent="true"
        android:background="@color/white"
        android:src="@drawable/transition"/>
    <ImageButton
        android:id="@+id/imageButtonFacebook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_centerInParent="false"
        android:layout_marginTop="30dp"
        android:layout_weight="1"
        android:background="@color/white"
        android:src="@drawable/fb"/>
    <View
        android:id="@+id/lineOpen"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/imageButtonMenu"
        android:layout_toRightOf="@+id/imageButtonThink"
        android:background="@drawable/line_rotate_horizontal"/>
    <ImageButton
        android:id="@+id/imageButtonGooglePlus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="25dp"
        android:layout_weight="1"
        android:background="@color/white"
        android:src="@drawable/google"/>

    <ImageButton
        android:id="@+id/imageButtonMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/imageButtonGooglePlus"
        android:layout_centerVertical="true"
        android:layout_marginRight="25dp"
        android:layout_weight="1"
        android:background="@color/white"
        android:src="@drawable/menu"/>
        
    <View
        android:id="@+id/lineGoogleplus"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/lineOpen"
        android:layout_toLeftOf="@+id/imageButtonThink"
        android:layout_toRightOf="@id/imageButtonGooglePlus"
        android:background="@drawable/line_rotate_horizontal"/>
    <View
        android:id="@+id/lineBottom"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_above="@+id/imageButtonAbout"
        android:layout_alignParentLeft="false"
        android:layout_below="@+id/imageButtonThink"
        android:layout_centerHorizontal="true"
        android:background="@drawable/line_rotate_vertical"/>
    <ImageButton
        android:id="@+id/imageButtonAbout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageButtonFacebook"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="30dp"
        android:layout_weight="1"
        android:background="@color/white"
        android:src="@drawable/about"/>
    <View
        android:id="@+id/lineFacebook"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:layout_above="@+id/imageButtonThink"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="false"
        android:layout_below="@+id/imageButtonFacebook"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="false"
        android:background="@drawable/line_rotate_vertical"/>
    <ImageButton
        android:id="@+id/imageButtonTwitter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageButtonLocation"
        android:layout_below="@+id/imageButtonInstagram"
        android:layout_weight="1"
        android:background="@color/white"
        android:src="@drawable/twitter"/>
    <ImageButton
        android:id="@+id/imageButtonInstagram"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButtonFacebook"
        android:layout_toRightOf="@+id/imageButtonMerch"
        android:layout_weight="1"
        android:background="@color/white"
        android:src="@drawable/instagram"/>

    <ImageButton
        android:id="@+id/imageButtonKCs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageButtonLocation"
        android:layout_alignLeft="@+id/lineGoogleplus"
        android:layout_weight="1"
        android:background="@color/white"
        android:src="@drawable/www"/>
    <ImageButton
        android:id="@+id/imageButtonMerch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageButtonGooglePlus"
        android:layout_below="@+id/imageButtonTwitter"
        android:layout_marginLeft="12dp"
        android:layout_weight="1"
        android:background="@color/white"
        android:src="@drawable/merch"/>
    <ImageButton
        android:id="@+id/imageButtonLocation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageButtonAbout"
        android:layout_toLeftOf="@+id/imageButtonOpen"
        android:layout_weight="1"
        android:background="@color/white"
        android:src="@drawable/location"/>
    <ImageButton
        android:id="@+id/imageButtonOpen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageButtonKCs"
        android:layout_alignRight="@+id/imageButtonMenu"
        android:layout_marginRight="11dp"
        android:layout_weight="1"
        android:background="@color/white"
        android:src="@drawable/hours"/>
</RelativeLayout>